<!DOCTYPE html>
<html>
  <head>
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Import materialize.css-->
    <link rel="stylesheet" href="../materialize/css/materialize.min.css">

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  </head>
  <%- include('../header.ejs')  %>
    <div class="container-fluid background-master">
        <div class="card">
            <div class="card-header">
                <div class="row">
                    <div class="col s12">
                        <button  class="waves-effect waves-light btn-large" id="start-DaiHoi"><i class="material-icons right">cloud</i> Bắt đầu đại hội</button>
                        <button  class="waves-effect waves-light btn-large" id="start-DaiHoi"><i class="material-icons right">print</i> Suất file</button>
                    </div>
                </div>
            </div>
            <div class="card-content">
                <div class="row">
                    <div class="col s12">
                      <iframe width="100%" height="500px" src="https://docs.google.com/document/d/1V0Drkhfg5o4XYDRnDWj5It745PKlVBbdN2wM6YPrMTM/edit?usp=sharing"></iframe>
                    </div>
                  </div>
            </div>
        </div>
        
      
    </div>
    
    <!--JavaScript at end of body for optimized loading-->
    <script src="../materialize/js/jquery-3.4.1.min.js"></script>
    <script src="../materialize/js/materialize.min.js"></script>
    <script src="../materialize/js/customer.js"></script>
    <script>
        $('#start-DaiHoi').click(event => {
       event.preventDefault()
       var instance = M.Modal.getInstance($('.modal'));
       $.ajax({
         type:'get',
         url:'../ajax/bat-dau-dai-hoi',
         beforeSend:() => {
           instance.open()
         },
         complete: () => {
          instance.close()
         }
       })
     })
    </script>
  </body>
</html>